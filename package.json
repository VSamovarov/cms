{
  "name": "cms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npx nodemon",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && npx tsc",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix",
    "prettier": "npx prettier --write ./src",
    "test": "jest --setupFiles ./jest.config.json",
    "migrate:make": "knex --esm migrate:make --knexfile src/database/knexfile.ts -x ts",
    "migrate:latest": "knex --esm --knexfile ./src/database/knexfile.ts migrate:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "objection": "^3.0.1",
    "pg": "^8.10.0",
    "tsconfig-paths": "^4.2.0"
  }
}
